{
  "name": "dna-dashboard",
  "version": "0.0.0",
  "main": "dist/dna-dashboard/index.js",
  "repository": {},
  "scripts": {
    "ng": "ng serve",
    "clean": "rimraf dist",
    "electron": "wait-on http://localhost:4200 && electron .",
    "prestart": "npm run clean",
    "start": "concurrently -k \"ng serve\" \"npm run electron\"",
    "prebuild": "npm run clean",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prerelease": "npm run build -- --prod",
    "release": "electron-builder"
  },
  "build": {
    "appId": "com.dna.dashboard",
    "productName": "DNA Dashboard",
    "files": [
      "main.js",
      "dist/dna-dashboard"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.9",
    "@angular/cdk": "^11.1.2",
    "@angular/common": "~11.0.9",
    "@angular/compiler": "~11.0.9",
    "@angular/core": "~11.0.9",
    "@angular/forms": "~11.0.9",
    "@angular/material": "^11.1.2",
    "@angular/platform-browser": "~11.0.9",
    "@angular/platform-browser-dynamic": "~11.0.9",
    "@angular/router": "~11.0.9",
    "@ngrx/effects": "^11.0.1",
    "@ngrx/entity": "^11.0.1",
    "@ngrx/store": "^11.0.1",
    "@ngrx/store-devtools": "^11.0.1",
    "@ngxs/cli": "^2.0.0",
    "@ngxs/devtools-plugin": "^3.7.1",
    "@ngxs/logger-plugin": "^3.7.1",
    "@ngxs/store": "^3.7.1",
    "material": "^0.4.3",
    "material-design-icons": "^3.0.1",
    "material-design-icons-iconfont": "^6.1.0",
    "ngx-spinner": "^10.0.1",
    "ngxs": "^2.0.0-rc.20",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.7",
    "@angular/cli": "~11.0.7",
    "@angular/compiler-cli": "~11.0.9",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "concurrently": "^5.3.0",
    "electron": "^11.2.1",
    "electron-builder": "^22.9.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2",
    "wait-on": "^5.2.1"
  }
}
